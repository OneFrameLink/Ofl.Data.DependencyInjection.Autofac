<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Description>Ofl.Data.Abstractions implementation registrations in Autofac</Description>
    <TargetFramework>netstandard1.3</TargetFramework>
    <AssemblyName>Ofl.Data.DependencyInjection.Autofac</AssemblyName>
    <PackageId>Ofl.Data.DependencyInjection.Autofac</PackageId>
    <PackageProjectUrl>https://github.com/OneFrameLink/Ofl.Data.DependencyInjection.Autofac</PackageProjectUrl>
    <RepositoryUrl>https://github.com/OneFrameLink/Ofl.Data.DependencyInjection.Autofac.git</RepositoryUrl>
    <Version>2.0.0</Version>
    <PackageTags>adonet entity-framework entity-framework-core dependency-injection autofac</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Autofac.Extensions.DependencyInjection" Version="4.1.0" />
    <PackageReference Include="Microsoft.NETCore.Portable.Compatibility" Version="1.0.1" />
    <PackageReference Include="Ofl.Data" Version="2.0.9" />
    <PackageReference Include="Ofl.Data.Abstractions" Version="1.0.3" />
  </ItemGroup>

</Project>
